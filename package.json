    "homepage": "https://github.com/elegant-view/vtpl#readme",
    "devDependencies": {
        "babel-core": "^6.4.5",
        "babel-loader": "^6.2.1",
        "babel-plugin-transform-es2015-modules-amd": "^6.4.3",
        "babel-plugin-transform-es2015-typeof-symbol": "^6.4.3",
        "babel-preset-es2015": "^6.3.13",
        "edp": "^1.1.1",
        "edp-webserver": "^1.2.5",
